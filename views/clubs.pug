head
    title Clubs

    div(class = 'ui container')
        div(class='ui inverted segment')
            div(class='ui inverted secondary menu')
                a(class = 'item' href='/') Home
                a(class = 'item' href='/schools') Schools
                a(class = 'item' href='/students') Students
                a(class = 'item' href='/courses') Courses
                a(class = 'item' href='/clubs') Clubs

        div(class = 'float-container')
            each row in rows
                div(class='float-child')
                    div(class = 'ui card')
                        div(class = 'image')
                            img(src='../image/clubs.png')

                        div(class = 'content')
                            a(class='header' href='/clubs/' + row.club_id) #{row.club_name}
                            div(class = 'meta')
                                span(class='date') Club ID: #{row.club_id}
                                br
